{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "totalPayment": {
      "type": "decimal",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "addressShipping": {
      "type": "json",
      "required": true
    },
    "idPayment": {
      "type": "string",
      "required": true
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "productsPayment": {
      "type": "decimal",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "required": true
    },
    "delivered": {
      "type": "enumeration",
      "enum": [
        "Entregado",
        "Pendiente"
      ],
      "default": "Pendiente",
      "required": false
    }
  }
}
